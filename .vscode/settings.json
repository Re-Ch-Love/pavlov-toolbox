{
    "python.analysis.typeCheckingMode": "standard",
    "python.analysis.packageIndexDepths": [
        {
            "name": "PySide6",
            "depth": 2
        },
        {
            "name": "qfluentwidgets",
            "includeAllSymbols": true
        }
    ],
    "editor.formatOnSave": true,
    "black-formatter.args": [
        "--line-length",
        "100"
    ],
    "terminal.integrated.env.windows": {
        "PYTHONPATH": "${workspaceFolder}"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "venv": true,
        "**/__pycache__": true,
        "build": true,
        "dist": true,
    },
    "VSCodeCounter.exclude": [
        "**/.gitignore",
        "**/.vscode/**",
        "**/node_modules/**",
        "generated_ui"
    ],
    "cSpell.words": [
        "Frameless",
        "MEIPASS",
        "modfile",
        "pyside",
        "qconfig",
        "qfluentwidgets",
        "qframelesswindow",
        "qreq",
        "qrequest",
        "webengine"
    ]
}